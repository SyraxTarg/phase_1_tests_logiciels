// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlite"
  // url      = "file:./pokecenter.db"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}